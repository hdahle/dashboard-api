{
  "name": "eia",
  "version": "1.0.0",
  "description": "Get various energy data from EIA and store to Redis",
  "main": "eia.js",
  "scripts": {
    "gas": "node eia.js --series gas --apikey `cat eiakey.txt` --key eia-global-gas",
    "oil": "node eia.js --series oil --apikey `cat eiakey.txt` --key eia-global-oil",
    "coal": "node eia.js --series coal --apikey `cat eiakey.txt` --key eia-global-coal",
    "electricity": "node eia.js --series electricity --apikey `cat eiakey.txt` --key eia-global-electricity",
    "renewable-gen": "node eia.js --series renewable-gen --apikey `cat eiakey.txt` --key eia-global-renewable-gen",
    "nuclear-gen": "node eia.js --series nuclear-gen --apikey `cat eiakey.txt` --key eia-global-nuclear-gen",
    "fossilfuel-gen": "node eia.js --series fossilfuel-gen --apikey `cat eiakey.txt` --key eia-global-fossilfuel-gen",
    "gdp": "node eia.js --series gdp --apikey `cat eiakey.txt` --key eia-global-gdp",
    "population": "node eia.js --series population --apikey `cat eiakey.txt` --key eia-global-population",
    "emissions": "node eia.js --series  emissions --apikey `cat eiakey.txt` --key eia-global-emissions",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "H Dahle, Hamachi AS",
  "license": "MIT",
  "dependencies": {
    "minimist": "1.2.6",
    "moment": "2.29.4",
    "node-fetch": "2.6.7",
    "redis": "3.1.1"
  }
}